<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.customer.entities.Customer" table="Customer" schema="rieltor_company">
        <id name="idCustomer" column="idCustomer"/>
        <property name="agreeToDataProcessing" column="agreeToDataProcessing"/>
        <property name="comment" column="comment"/>
        <property name="dateTime" column="dateTime"/>
        <property name="email" column="email"/>
        <property name="idHouse" column="idHouse"/>
        <property name="mortgage" column="mortgage"/>
        <property name="name" column="name"/>
        <property name="phoneNumber" column="phoneNumber"/>
    </class>
</hibernate-mapping>